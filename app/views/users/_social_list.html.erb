    <ul class="social_list">
      <% users.each do |user| %>
          <li>
            <div class="avatar">
              <%= link_to user do %>
                  <%= image_tag user.avatar.url %>
              <% end %>
            </div>
            <div class="name">
              <%= link_to user.name, user %>
            </div>
            <div class="info">
              <span><%= h user.sex_text %></span>
              <span><%= h "(#{user.age}岁)" %></span>
            </div>
            <div class="city">
              <%=h "#{user.city.name}" %>
            </div>
            <div class="tags">
              <% user.tags.each do |tag| %>
                <span><%= tag.name %></span>
              <% end %>
            </div>
          </li>
      <% end %>
    </ul>

